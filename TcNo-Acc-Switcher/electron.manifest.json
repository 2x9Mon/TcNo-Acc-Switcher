{
  "executable": "TcNo-Acc-Switcher",
  "splashscreen": {
    "imageFile": "TcNoAccSw-Icon-100x100.png"
  },
  "name": "TcNo-Account-Switcher",
  "author": "Wesley Pyburn (TechNobo)",
  "singleInstance": true,
  "environment": "Production",
  "build": {
    "appId": "com.TcNo-Acc-Switcher.app",
    "productName": "TcNo-Acc-Switcher",
    "copyright": "Copyright Wesley Pyburn (TechNobo) Â© 2020",
    "buildVersion": "3.1.0",
    "compression": "maximum",
    "directories": {
      "output": "../../../bin/Desktop"
    },
    "extraResources": [
      {
        "from": "./bin",
        "to": "bin",
        "filter": [ "**/*" ]
      }
    ],
    "files": [
      {
        "from": "./obj/desktop/win/node_modules",
        "to": "ElectronHostHook/node_modules",
        "filter": [ "**/*" ]
      },
      "**/*"
    ]
  }
}